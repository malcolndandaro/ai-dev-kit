{
    "version": "1.0",
    "description": "Skill activation triggers for Claude Code. Controls when skills automatically suggest or block actions.",
    "skills": {
        "spark-declarative-pipelines": {
            "type": "domain",
            "enforcement": "suggest",
            "priority": "high",
            "description": "Build Spark Declarative Pipelines (SDP/LDP/DLT)",
            "promptTriggers": {
                "keywords": [
                    "pipeline",
                    "dlt",
                    "delta live tables",
                    "sdp",
                    "ldp",
                    "streaming table",
                    "materialized view",
                    "medallion",
                    "bronze",
                    "silver",
                    "gold",
                    "auto loader",
                    "autoloader",
                    "cdc",
                    "change data capture"
                ],
                "intentPatterns": [
                    "(create|build|implement|add|update).*?(pipeline|dlt|streaming table|materialized view)",
                    "(medallion|lakehouse).*?(architecture|pattern)",
                    "(ingest|load).*?(data|files|csv|json|parquet)"
                ]
            }
        },
